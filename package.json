{
  "name": "mdquiz",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/mdquiz.js",
  "files": [
    "demo",
    "dist",
    "src/*",
    "tsconfig.json",
    "vite.config-lib.ts",
    ".npmignore"
  ],
  "exports": {
    ".": {
      "buildQuiz": "./dist/mdquiz"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && vite build -c vite.config-lib.ts",
    "build-demo": "tsc && vite build -c vite.config-demo.ts && vite preview --open",
    "dev": "npm run build && vite build -c vite.config-demo.ts && vite preview --open -c vite.config-demo.ts",
    "lint": "eslint .",
    "prepare": "npm run build",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@prantlf/jsonlint": "^16.0.0",
    "classnames": "^2.5.1",
    "fast-deep-equal": "^3.1.3",
    "globals": "^16.0.0",
    "highlight.js": "^11.11.1",
    "mobx": "^6.13.7",
    "mobx-react": "npm:mobx-react-lite@^4.1.0",
    "object-hash": "^3.0.0",
    "preact": "^10.26.4",
    "pyret-embed": "^0.0.1",
    "react": "npm:@preact/compat@^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "npm:@preact/compat@^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-loader-spinner": "^6.1.6",
    "react-showdown": "^2.3.1",
    "typescript": "5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-singlefile": "^2.2.0",
    "vite-plugin-static-copy": "^2.3.0"
  },
  "overrides": {
    "react-loader-spinner": {
      "react": "npm:@preact/compat@^18.3.1",
